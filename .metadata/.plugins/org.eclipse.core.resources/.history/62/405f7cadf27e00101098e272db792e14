package entity;

import java.time.LocalDateTime;

public class Compromisso {
	
	private LocalDateTime data;
	private String conteudo;
	
	public Compromisso() {
	}
	
	public Compromisso(LocalDateTime data, String conteudo) {
		super();
		this.data = data;
		this.conteudo = conteudo;
	}
	
	

}
